        var maxValue = lstValue.Max();
        var minValue = lstValue.Min();
        int maxIndex = 0;
        int minIndex = 0;
        var cnt = lstValue.Count();

        maxValue = 0;
        for (var i = 0; i < cnt; i++)
        {
            if (lstValue[i] > maxValue)
            {
                maxValue = lstValue[i];
                maxIndex = i + 1;
            }
        }
        minValue = 100;
        for (var i = 0; i < cnt; i++)
        {
            if (lstValue[i] < minValue && lstValue[i] > 0)
